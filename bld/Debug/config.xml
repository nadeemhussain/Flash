<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.Flash" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Flash</name>
  <description>FLash the saviour in dark times</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">nadeem hussain shaikh</author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="Orientation" value="portrait" />
  <preference name="Fullscreen" value="true" />
  <vs:plugin name="nl.x-services.plugins.flashlight" version="2.0.2" />
  <vs:plugin name="org.apache.cordova.splashscreen" version="0.3.3" />
  <vs:platformSpecificValues />
</widget>